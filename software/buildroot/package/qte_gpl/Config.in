config BR2_PACKAGE_QTE_GPL
	bool "Qt/Embedded (GPL version)"
	default n
	select BR2_INSTALL_LIBSTDCPP
	select BR2_PTHREADS_NATIVE
	select BR2_PACKAGE_JPEG
	select BR2_PACKAGE_LIBPNG
	select BR2_PACKAGE_E2FSPROGS
	help
	  Trolltech's Qt/Embedded GPL edition and Opie.
          Offers the same APIs used in building desktop applications tailored to a smaller footprint.

	  http://trolltech.com/

	  Only tested with threading on, PACKAGE_JPEG, PACKAGE_LIBPNG, but remove those if you like.

choice
        prompt "Qt/Embedded Version"
        default BR2_QTE_GPL_VERSION_2_3_10
	depends BR2_PACKAGE_QTE_GPL
        help
          Select which version of Qt/Embedded you wish to use.

#        config BR2_QTE_VERSION_2_3_8
#                bool "Qt/E 2.3.8"

#        config BR2_QTE_VERSION_2_3_9
#                bool "Qt/E 2.3.9"

        config BR2_QTE_GPL_VERSION_2_3_10
                bool "Qt/E 2.3.10"

#        config BR2_QTE_VERSION_2_3_11
#                bool "Qt/E 2.3.11"

	config BR2_QTE_GPL_VERSION_4_0_1
		bool "Qt/E 4.0.1"
endchoice

config BR2_QTE_GPL_VERSION
        string
	default "2.3.8"		if BR2_QTE_GPL_VERSION_2_3_8
	default "2.3.9"		if BR2_QTE_GPL_VERSION_2_3_9
	default "2.3.10"	if BR2_QTE_GPL_VERSION_2_3_10
	default "2.3.11"	if BR2_QTE_GPL_VERSION_2_3_11
	default "4.0.1"		if BR2_QTE_GPL_VERSION_4_0_1

config BR2_PACKAGE_QTE_GPL_QVFB
	bool "QVfb"
	default y
	depends BR2_PACKAGE_QTE_GPL
	help
	  Trolltech provides a Virtual Framebuffer used to simulate the embedded device during development.
	  If you activate it, you will have all you need to test your Qt/E code on your host developmen system.

	  http://trolltech.com/

choice
        prompt "QVfb Version"
        default BR2_QTE_GPL_QVFB_VERSION_2_3_2
	depends BR2_PACKAGE_QTE_GPL_QVFB
        help
          Select the version of QVfb you wish to use.

        config BR2_QTE_GPL_QVFB_VERSION_2_3_2
                bool "QVfb 2.3.2"

        config BR2_QTE_GPL_QVFB_VERSION_2_3_9
                bool "QVfb 2.3.9"

endchoice

config BR2_QTE_GPL_QVFB_VERSION
        string
	default "2.3.2"		if BR2_QTE_GPL_QVFB_VERSION_2_3_2
	default "2.3.9"		if BR2_QTE_GPL_QVFB_VERSION_2_3_9



config BR2_PACKAGE_QTE_GPL_OPIE
	bool "Opie"
	default n
	depends BR2_PACKAGE_QTE_GPL
	help
	  Open Palmtop Integrated Environment aka Opie

          Opie is a completely Open Source based graphical user environment for PDAs and other devices running Linux.
	  http://opie.handhelds.org/

choice
        prompt "Opie Version"
        default BR2_QTE_GPL_OPIE_VERSION_1_2_0
	depends BR2_PACKAGE_QTE_GPL_OPIE
        help
          Select the version of Opie you wish to use.

        config BR2_QTE_GPL_OPIE_VERSION_1_0_3
                bool "Opie 1.0.3"
        config BR2_QTE_GPL_OPIE_VERSION_1_2_0
                bool "Opie 1.2.0"

endchoice

config BR2_QTE_GPL_OPIE_VERSION
        string
	default "1.0.3"		if BR2_QTE_GPL_OPIE_VERSION_1_0_3
	default "1.2.0"		if BR2_QTE_GPL_OPIE_VERSION_1_2_0

config BR2_QTE_GPL_TMAKE_VERSION
        string
	default "1.13"

choice
	prompt "Install directory for Qt/E"
	default BR2_QTE_GPL_TARGET_DIR
	depends BR2_PACKAGE_QTE_GPL
	help
	  Choose installation directory for Qt/E library

	config BR2_QTE_GPL_TARGET_DIR
		bool "Target rootfs"
	config BR2_QTE_GPL_NFS_DIR
		bool "Host nfs export dir"
endchoice

config BR2_QTE_GPL_LIB_INSTALL_DIR
	string "Install dir name (a /lib dir will be created)"
	default "/usr" 		if BR2_QTE_GPL_TARGET_DIR
	default "/local/export" if BR2_QTE_GPL_NFS_DIR

